{
  "name": "violinist-dev/licence-check",
  "description": "License check for self hosted violinist",
  "autoload": {
    "psr-4": {"violinist\\LicenceCheck\\": "src"}
  },
  "autoload-dev": {
    "psr-4": {"violinist\\LicenceCheck\\Tests\\": "tests"}
  },
  "require": {
      "spomky-labs/base64url": "^2.0",
      "starkbank/ecdsa": "^2.0",
      "rybakit/msgpack": "^0.9.1",
      "simplito/elliptic-php": "^1.0",
      "ext-zlib": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "^8||^9",
      "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "^3.10",
    "slevomat/coding-standard": "^8.15"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
